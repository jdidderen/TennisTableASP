                                    break;
                                case 3:
                                    <td>@Model.Matchs.JVr3.Nom @Html.Raw(" ") @Model.Matchs.JVr3.Prenom</td>
                                    break;
                                case 4:
                                    <td>@Model.Matchs.JVr4.Nom @Html.Raw(" ") @Model.Matchs.JVr4.Prenom</td>
                                    break;
                            }

                            @{string[] scores = null;
                                if (x.Score != null)
                                {
                                    scores = Regex.Split(x.Score, @"\D+");
                                }
                            }
                            @for (int cpt = 0; cpt < 10; cpt++)
                    {
                        if (scores != null && cpt < scores.Length && scores[cpt].Length != 0)
                        {
                                    <td tabindex="@x.Sr.Ordre + cpt">
                                        @Html.TextBox("Set" + x.Sr.Ordre + cpt, scores[cpt], new
                           {
                               @maxlength = "2",
                               @class = "form-control valid span1",
                               @data_val = "true",
                               @data_val_regex = "Seulement des nombres",
                               @data_val_regex_pattern = "^\\d{1,2}$"
                           })
                                    </td>

                                }
                                else
                                {
                                    <td tabindex="@x.Sr.Ordre + cpt">
                                        @Html.TextBox("Set" + x.Sr.Ordre + cpt, 0, new { @maxlength = "2", @class = "span1" })
                                    </td>
                                }
                            }
                        </tr>
                                }
                </tbody>
            </table>
